{
  "plugins": ["react"],
  "extends": [
	  "plugin:react/recommended"
	],
  "parser": "babel-eslint",
  "rules": {
	  "no-console": "off",
	  "no-empty-function": "off",
		"operator-linebreak": "off",
		"react/display-name": "off",
		"react/prop-types": 0
  },
  "parserOptions": {
	  "ecmaVersion": 8
	},
  "globals": {
    "$": true,
    "__inline": true,
    "__uri": true
  }
}