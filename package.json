    "build-dev-server": "better-npm-run build-dev-server",
    "build-prod": "better-npm-run build-prod",
    "start-dev": "better-npm-run start-dev",
    "start-prod": "better-npm-run start-prod"
  },
  "betterScripts": {
    "build-dev-server": {
      "command": "webpack --colors --progress --config ./webpack.config.dev.server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start-dev": {
      "command": "node ./server/bin/start.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build-prod": {
      "command": "webpack --colors --config ./webpack.config.prod.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start-prod": {
      "command": "node ./server/bin/start.js",
