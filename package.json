  },
  "scripts": {
    "dev": "npm-run-all start-dev start-server",
    "build": "npm-run-all --sequential clean build-prod start-server",
    "clean": "rimraf ./client/dist",
    "build-dev-server": "better-npm-run build-dev-server",
    "build-prod": "better-npm-run build-prod",
    "start-dev": "better-npm-run start-dev",
    "start-server": "better-npm-run start-server"
  },
  "betterScripts": {
    "build-dev-server": {
      "command": "webpack --colors --progress --config ./webpack.config.dev.server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start-dev": {
      "command": "node node_modules/srv-cli/build/srv ./server/bin/start.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build-prod": {
      "command": "webpack --colors --config ./webpack.config.prod.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start-server": {
      "command": "pm2 start ./server/apps.json",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "license": "MIT"
}
